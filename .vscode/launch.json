{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "C Debug (Raspberry Pi)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/Repository/bin/Server",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": false
                }
            ],
            "preLaunchTask": "build",
            "sourceFileMap": {
                "/Repository/src/": "${workspaceFolder}"
            }
        }
    ]
}